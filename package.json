{
  "name": "starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "lint": "npx eslint --ext js,ts,tsx",
    "lint:fix": "npx eslint --ext js,ts,tsx --fix",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "prettier:fix": "prettier --loglevel warn --write 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "cd .. && husky install ui/.husky",
    "generate:api": "openapi-rq -i ./src/core/api/fixtures/*.json -o ./src/core/api"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.66.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "motion": "^12.4.2",
    "nanoid": "^5.0.9",
    "reablocks": "^8.7.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.5",
    "react-use": "^17.6.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^1.6.2",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.20.0",
    "@storybook/addon-actions": "^8.5.4",
    "@storybook/addon-docs": "^8.5.4",
    "@storybook/addon-essentials": "^8.5.4",
    "@storybook/addon-interactions": "^8.5.4",
    "@storybook/addon-mdx-gfm": "^8.5.4",
    "@storybook/addon-onboarding": "^8.5.4",
    "@storybook/addon-storysource": "^8.5.4",
    "@storybook/addon-themes": "^8.5.4",
    "@storybook/blocks": "^8.5.4",
    "@storybook/manager-api": "^8.5.4",
    "@storybook/preview-api": "^8.5.4",
    "@storybook/react": "^8.5.4",
    "@storybook/react-vite": "^8.5.4",
    "@storybook/test": "^8.5.4",
    "@tanstack/eslint-plugin-query": "^5.66.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.5.0",
    "storybook": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}": [
      "prettier --write "
    ]
  }
}
