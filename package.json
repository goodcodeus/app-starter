{
  "name": "starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "lint": "npx eslint --ext js,ts,tsx",
    "lint:fix": "npx eslint --ext js,ts,tsx --fix",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "prettier:fix": "prettier --loglevel warn --write 'src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}'",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "cd .. && husky install ui/.husky"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@hookform/resolvers": "^3.3.2",
    "apollo-link-rest": "^0.9.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "framer-motion": "7.8.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "nanoid": "^5.0.3",
    "reablocks": "^5.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.1",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.15.0",
    "react-use": "^17.4.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-mdx-gfm": "^7.5.3",
    "@storybook/addon-storysource": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "storybook": "^7.5.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "vite-plugin-svgr": {
      "vite": "^5.0.2"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,md,graphql,gql}": [
      "prettier --write "
    ]
  }
}
